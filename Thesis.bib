@article{Fowler2007,
abstract = {The circulation of the ocean is usually divided into two parts, a wind-driven circulation that dominates in the upper few hundred meters, and a density-driven circulation that dominates below. The latter is called the ‘thermohaline' circulation because of the role of heating, cool- ing, freshening, and salinification in producing regional density differences within the ocean. The thermohaline circulation, for the most part, is an ‘overturning' circulation in which warm water flows poleward near the surface and is subsequently converted into cold water that sinks and flows equatorward in the in- terior. Radiocarbon measurements show that the thermohaline circulation turns over all the deep water in the ocean every 600 years or so},
