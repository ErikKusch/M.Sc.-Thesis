journal = {Remote Sensing},
keywords = {Biomass,Mediterranean environment,Nitrogen,Remote sensing,Vegetation indices,Wheat},
month = {mar},
number = {4},
pages = {2827--2844},
title = {{Assessing the Robustness of Vegetation Indices to Estimate Wheat N in Mediterranean Environments}},
url = {http://www.mdpi.com/2072-4292/6/4/2827},
volume = {6},
