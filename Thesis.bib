file = {:C$\backslash$:/Users/Erik Kusch/Documents/Literature (Mendeley)/Pinzon, Tucker - 2014 - A non-stationary 1981-2012 AVHRR NDVI3g time series.pdf:pdf},
isbn = {9781461431022},
issn = {2072-4292},
journal = {Remote Sensing},
keywords = {Advanced very high resolution radiometer (AVHRR),Bayesian analysis,Bias,Climate variability,Non-stationary,Normalized difference vegetation index (NDVI),Uncertainty},
month = {jul},
number = {8},
pages = {6929--6960},
title = {{A Non-Stationary 1981–2012 AVHRR NDVI3g Time Series}},
url = {http://www.mdpi.com/2072-4292/6/8/6929},
volume = {6},
