keywords = {D 04002 Surveying and remote sensing,D 04100 Terrestrial ecosystems -,D 04625 Plants - general,general,phytosociology,remote sensing,vegetation,vegetation patterns},
month = {jan},
number = {1},
pages = {39--48},
pmid = {192},
title = {{A remote sensing based vegetation classification logic for global land cover analysis}},
