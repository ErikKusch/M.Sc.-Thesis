@article{Muthukrishnan2016,
abstract = {1. Alternate stable states (ASS) theory is a dominant conceptual framework for understanding processes that support resilience of ecological communities in the face of multiple anthropogenic disturbances. For decades, coral reefs have been cited as a classic example of ASS, yet this position remains highly controversial, largely because convincing empirical evaluations have been elusive.2.Using a combination of empirical measurements of positive feedback processes and simulation modeling, we assessed ASS in coral reefs of the Eastern Tropical Pacific (ETP) by identifying the potential for multiple basins of attraction from a mechanistic perspective. 3.Using bioassays of algal growth and consumption we quantified two spatially-localised positive feedback mechanisms on coral reefs: increasing herbivore activity associated with higher coral abundances and amelioration of nutrient limitation leading to higher algal growth in areas with higher algal abundance. 4. Analysis of a model of benthic community dynamics incorporating these feedbacks showed they were central to producing hysteresis and bistability in the model, two hallmarks of ASS. Further, reefs simulated with conditions matching those measured at Isla Contadora, Panam{\'{a}} displayed ASS dynamics while producing spatial patterns matching field observations. This provides model-derived support, based on empirically measured conditions of ETP reefs, for the presence of ASS. 5. Synthesis. The combination of experimental bioassays to measure feedback strengths and simulation models to evaluate the influence of those feedbacks provides a novel, non-destructive approach to evaluating ASS dynamics that can be applied in threatened ecosystems where classic approaches are not viable.},
author = {Muthukrishnan, Ranjan and Lloyd-Smith, James O. and Fong, Peggy},
doi = {10.1111/1365-2745.12631},
file = {:C$\backslash$:/Users/Erik Kusch/Documents/Literature (Mendeley)/Muthukrishnan, Lloyd-Smith, Fong - 2016 - Mechanisms of resilience Empirically quantified positive feedbacks produce alternate stable st.pdf:pdf},
issn = {13652745},
journal = {Journal of Ecology},
keywords = {Algae,Alternate stable states,Coral reef,Determinants of plant community diversity and stru,Herbivory,Nutrients,Positive feedbacks},
pages = {1662--1672},
title = {{Mechanisms of resilience: Empirically quantified positive feedbacks produce alternate stable states dynamics in a model of a tropical reef}},
year = {2016}
}
