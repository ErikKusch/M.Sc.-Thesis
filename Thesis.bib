journal = {Ecosystems},
keywords = {AVHRR,Landsat,TM,conterminous united-states,ecology,indexes,land use,landscapes,pattern,scale,spatial heterogeneity,topography,vegetation indices},
month = {may},
number = {3},
pages = {268--282},
title = {{Analysis of Large-Scale Spatial Heterogeneity in Vegetation Indices among North American Landscapes}},
url = {http://link.springer.com/10.1007/s100219900021},
volume = {1},
year = {1998}
}
