pmid = {15519965},
title = {{Global climate and the distribution of plant biomes}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15519965{\%}5Cnhttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC1693431 http://www.royalsocietypublishing.org/doi/10.1098/rstb.2004.1525},
