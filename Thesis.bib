file = {:C$\backslash$:/Users/Erik Kusch/Documents/Literature (Mendeley)/Xu et al. - 2013 - Temperature and vegetation seasonality diminishment over northern lands.pdf:pdf},
isbn = {1758-678X},
issn = {1758-678X},
journal = {Nature Climate Change},
month = {jun},
