isbn = {1365-2745},
issn = {00220477},
journal = {Journal of Ecology},
keywords = {CO 2,Climate change,Drought,Environmental change,Mortality,Plant-climate interactions,Regime shifts,Review,Spatio-temporal scales,Vulnerability},
month = {jan},
