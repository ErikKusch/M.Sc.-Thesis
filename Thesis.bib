eprint = {0608246v3},
file = {:C$\backslash$:/Users/Erik Kusch/Documents/Literature (Mendeley)/Rudgers et al. - 2018 - Climate sensitivity functions and net primary production A framework for incorporating climate mean and variabil.pdf:pdf},
isbn = {0000000154871},
issn = {00129658},
journal = {Ecology},
keywords = {Bouteloua,Chihuahuan Desert,Great Plains grassland,Shortgrass steppe,Standardized Precipitation Evapotranspiration Inde,climate sensitivity function,climate variability,creosote shrubland,ecotone dynamics,net primary production},
month = {mar},
