editor = {Wilson, Scott},
file = {:C$\backslash$:/Users/Erik Kusch/Documents/Literature (Mendeley)/Laughlin - 2014 - The intrinsic dimensionality of plant traits and its relevance to community assembly(2).pdf:pdf;:C$\backslash$:/Users/Erik Kusch/Documents/Literature (Mendeley)/Laughlin - 2014 - The intrinsic dimensionality of plant traits and its relevance to community assembly.pdf:pdf},
isbn = {1365-2745},
issn = {00220477},
journal = {Journal of Ecology},
keywords = {Community assembly,Curse of dimensionality,Determinants of plant community diversity and stru,Ecosystem processes,Intrinsic dimension,Isomap,Nonlinear data reduction,Plant spectrums,Plant strategies,Plant traits},
month = {jan},
