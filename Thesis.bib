file = {:C$\backslash$:/Users/Erik Kusch/Documents/Literature (Mendeley)/Stow et al. - 2004 - Remote sensing of vegetation and land-cover change in Arctic Tundra Ecosystems.pdf:pdf},
isbn = {0034-4257},
