file = {:C$\backslash$:/Users/Erik Kusch/Documents/Literature (Mendeley)/Simard et al. - 2011 - Mapping forest canopy height globally with spaceborne lidar.pdf:pdf},
isbn = {2156-2202},
issn = {0148-0227},
journal = {Journal of Geophysical Research},
keywords = {doi:10.102,http://dx.doi.org/10.1029/2011JG001708},
month = {nov},
